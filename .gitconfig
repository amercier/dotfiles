  email = pro.alexandre.mercier@gmail.com
[push]
  default = simple
[remote "origin"]
  fetch = +refs/pull/*/head:refs/remotes/origin/pr/*
[color]
  ui = auto
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = green
  local = cyan
  remote = magenta
[color "diff"]
  meta = yellow
  frag = bold
  new = green
  old = red
  whitespace = white reverse
[color "status"]
  header = normal
  added = green
  updated = green
  changed = red
  untracked = blue
  unmerged = red
  branch = cyan
  nobranch = bold

