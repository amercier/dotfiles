[user]
  name = Alex Mercier
  email = pro.alexandre.mercier@gmail.com
[push]
  default = simple
[remote "origin"]
  fetch = +refs/pull/*/head:refs/remotes/origin/pr/*
[color]
  ui = auto
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = green black
  local = cyan black
  remote = magenta black
[color "diff"]
  meta = yellow black
  frag = black black bold
  new = green black
  old = red black
  whitespace = white reverse
[color "status"]
  header = normal black
  added = green black
  updated = green black
  changed = red black
  untracked = blue black
  unmerged = red black
  branch = cyan black
  nobranch = black black bold

