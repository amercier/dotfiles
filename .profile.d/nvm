NVM_DIR="$HOME/.nvm"
if [ -e "$NVM_DIR/nvm.sh" ]; then
  export NVM_DIR
  . "$NVM_DIR/nvm.sh"
  path_add "$NVM_DIR/current/bin"
  path_add "./node_modules/.bin"

  function runn() {
    args="$@"
    $args
    code=$?
    if [ $code == 0 ]; then
      msg="SUCCESS"
    else
      msg="FAILURE - Exited with $code"
    fi
    notify -t "$args" -m "$msg" >/dev/null 2>&1
    return $code
  }
fi

