if [ "$(brew --prefix docker-machine 2>/dev/null)" != "" ]; then
  name=$(docker-machine ls | head -n 2 | tail -n 1 | awk '{ print $1; }')
  if [ "$name" != "" ]; then
    if [ "$(docker-machine status $name)" != "Running" ]; then
      docker-machine start $name
    fi
    eval $(docker-machine env $name)
  fi
fi
